                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "user_email",
                                    "user_password"
                                ],
                                "properties": {
                                    "user_email": {
                                        "type": "email",
                                        "example": "example@beachcomber.com"
                                    },
                                    "user_password": {
                                        "type": "password",
                                        "example": "beachcomber"
                                    }
                                },
                                "type": "object"
                            }
                        },
                        "application/json": {
                            "schema": {}
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login Successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "access_token": {
                                            "type": "string",
                                            "example": "string"
                                        },
                                        "token_type": {
                                            "type": "string",
                                            "example": "string"
                                        },
                                        "expires_in": {
                                            "type": "integer",
                                            "example": "360"
                                        },
                                        "user": {
